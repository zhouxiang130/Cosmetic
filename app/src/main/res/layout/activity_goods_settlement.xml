                        />

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dis0.5"
                    android:background="@color/CE7_E7_E7"/>

                <RelativeLayout
                    android:id="@+id/settlement_goods_cb_rl1"
