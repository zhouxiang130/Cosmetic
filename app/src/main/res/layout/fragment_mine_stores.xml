<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/CF8_F8_F8"
              android:gravity="center"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/CF8_F8_F8"
        android:orientation="vertical">

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis6"
            android:background="@color/CF8_F8_F8"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CE7_E7_E7"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CF5_F5_F5"/>

        <com.yj.cosmetics.widget.MaterialRippleLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/mine_personal_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="@dimen/dis15"
                android:paddingBottom="@dimen/dis15">

                <ImageView
                    android:id="@+id/image_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:src="@mipmap/dizhi"/>

                <ImageView
                    android:id="@+id/mine_personal_iv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dis15"
                    android:src="@mipmap/dianhua"/>

                <TextView
                    android:id="@+id/tv_stores_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:layout_marginRight="@dimen/dis10"
                    android:layout_toLeftOf="@id/mine_personal_iv1"
                    android:layout_toRightOf="@id/image_icon"
                    android:ellipsize="end"
                    android:maxLines="2"
                    android:text=""
                    android:textColor="@color/C64_64_64"
                    android:textSize="@dimen/font15"/>
            </RelativeLayout>
        </com.yj.cosmetics.widget.MaterialRippleLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CE7_E7_E7"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis6"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CE7_E7_E7"/>

        <com.yj.cosmetics.widget.MaterialRippleLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/mine_personal_leaguer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="@dimen/dis15"
                android:paddingBottom="@dimen/dis15">

                <ImageView
                    android:id="@+id/image_icon1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:src="@mipmap/fuwu"/>

                <TextView
                    android:id="@+id/tv_store_services_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:layout_marginRight="@dimen/dis20"
                    android:layout_toRightOf="@id/image_icon1"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text=""
                    android:textColor="@color/C64_64_64"
                    android:textSize="@dimen/font15"/>
            </RelativeLayout>
        </com.yj.cosmetics.widget.MaterialRippleLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CF5_F5_F5"/>

        <com.yj.cosmetics.widget.MaterialRippleLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/mine_personal_tel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="@dimen/dis15"
                android:paddingBottom="@dimen/dis15">

                <ImageView
                    android:id="@+id/image_icon2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:src="@mipmap/shijian"/>

                <TextView
                    android:id="@+id/tv_store_services_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:layout_marginRight="@dimen/dis20"
                    android:layout_toRightOf="@id/image_icon2"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text=""
                    android:textColor="@color/C64_64_64"
                    android:textSize="@dimen/font15"/>
            </RelativeLayout>
        </com.yj.cosmetics.widget.MaterialRippleLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CE7_E7_E7"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis6"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CE7_E7_E7"/>

        <com.yj.cosmetics.widget.MaterialRippleLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/mine_personal_alipay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:paddingTop="@dimen/dis18"
                android:paddingBottom="@dimen/dis15">

                <ImageView
                    android:id="@+id/image_icon3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:src="@mipmap/manjian"/>

                <TextView
                    android:id="@+id/tv_store_services_content"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dis10"
                    android:layout_marginRight="@dimen/dis20"
                    android:layout_toRightOf="@id/image_icon3"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:text=""
                    android:textColor="@color/C64_64_64"
                    android:textSize="@dimen/font15"/>
            </RelativeLayout>
        </com.yj.cosmetics.widget.MaterialRippleLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dis0.5"
            android:background="@color/CE7_E7_E7"/>
    </LinearLayout>
</LinearLayout>